#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/lerna@6.6.2/node_modules/lerna/dist/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/lerna@6.6.2/node_modules/lerna/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/lerna@6.6.2/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/lerna@6.6.2/node_modules/lerna/dist/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/lerna@6.6.2/node_modules/lerna/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/lerna@6.6.2/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lerna/dist/cli.js" "$@"
else
  exec node  "$basedir/../lerna/dist/cli.js" "$@"
fi
