#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/forever@4.0.3/node_modules/forever/bin/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/forever@4.0.3/node_modules/forever/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/forever@4.0.3/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/forever@4.0.3/node_modules/forever/bin/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/forever@4.0.3/node_modules/forever/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/forever@4.0.3/node_modules:/Users/daniel_spinu/Documents/EFREI/M2/Frontend/TP/graded-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../forever/bin/forever" "$@"
else
  exec node  "$basedir/../forever/bin/forever" "$@"
fi
